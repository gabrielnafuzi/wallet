{
  "name": "wish-wallet",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite --open",
    "gen:theme-typings": "chakra-cli tokens ./src/styles/theme",
    "lint:fix": "pnpm lint --fix",
    "lint": "eslint src --max-warnings=0",
    "postinstall": "pnpm gen:theme-typings",
    "prepare": "husky install",
    "preview": "vite preview",
    "preview:test": "start-server-and-test preview http://localhost:4173",
    "test:ci": "jest --runInBand",
    "test:e2e": "pnpm preview:test 'cypress open'",
    "test:e2e:ci": "vite build && pnpm preview:test 'cypress run'",
    "test:e2e:headless": "pnpm preview:test 'cypress run'",
    "test:watch": "jest --watch",
    "test": "jest",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
  "lint-staged": {
    "src/**/*": [
      "pnpm lint:fix"
    ]
  },
  "engines": {
    "node": "16.x || 14.x"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.0.0-next.0",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@tanstack/react-location": "^3.7.4",
    "framer-motion": "^6.3.3",
    "nanoid": "^3.3.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "regenerator-runtime": "^0.13.9",
    "yup": "^0.32.11",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.0.0",
    "@chakra-ui/styled-system": "^2.2.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.2",
    "cypress": "^9.6.1",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "git-commit-msg-linter": "^4.1.2",
    "husky": "^8.0.1",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "^28.0.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.8"
  }
}
